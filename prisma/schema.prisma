datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Products {
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  title    String
  isNew   Boolean
  oldPrice Float
  price    Float
  description   String
  category    String
  image    String
}
